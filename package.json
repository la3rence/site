{
  "name": "blog",
  "version": "1.0.0",
  "markdown": "github",
  "scripts": {
    "feed": "node ./lib/feed.mjs > ./public/atom.xml",
    "dev": "pnpm feed && next dev -H 0.0.0.0",
    "start": "next start",
    "build": "NEXT_PUBLIC_BUILDTIME=$(date '+%s') next build && pnpm feed && next-sitemap",
    "lint": "next lint",
    "export": "next export",
    "fmt": "prettier --write .",
    "clean": "rm -rf .next; rm -rf node_modules"
  },
  "dependencies": {
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@microsoft/fetch-event-source": "2.0.1",
    "@vercel/og": "0.5.17",
    "dotenv": "16.3.1",
    "flexsearch": "^0.7.31",
    "gray-matter": "4.0.3",
    "hast-util-from-html": "^2.0.0",
    "hast-util-to-jsx-runtime": "^2.1.1",
    "highlight.js": "11.8.0",
    "markdown-it": "13.0.2",
    "mongodb": "^6.0.0",
    "next": "13.5.1",
    "next-sitemap": "^4.0.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-star-ratings": "^2.3.0",
    "react-tweet": "3.1.1",
    "rehype-autolink-headings": "7.0.0",
    "rehype-external-links": "3.0.0",
    "rehype-highlight": "7.0.0",
    "rehype-slug": "6.0.0",
    "rehype-stringify": "9.0.4",
    "rehype-video": "1.2.2",
    "remark": "14.0.3",
    "remark-gfm": "3.0.1",
    "remark-rehype": "11.0.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.10",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 80
  },
  "engines": {
    "node": "^15.0.0 || >=16.0.0"
